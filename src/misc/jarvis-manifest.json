{
    "_metadata": {
        "major_version": 1,
        "minor_version": 1
    },
    "display_information": {
        "name": "jarvis"
    },
    "features": {
        "bot_user": {
            "display_name": "jarvis",
            "always_online": false
        }
    },
    "oauth_config": {
        "scopes": {
            "user": [
                "chat:write",
                "channels:read"
            ],
            "bot": [
                "app_mentions:read",
                "channels:history",
                "chat:write",
                "im:history",
                "incoming-webhook",
                "chat:write.public",
                "chat:write.customize",
                "channels:read",
                "users:read",
                "users:write"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "bot_events": [
                "message.im"
            ]
        },
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}
